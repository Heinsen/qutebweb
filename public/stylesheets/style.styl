.navbar-logo
	position absolute
	max-height 80px
	padding 10px

.navbar
	background-color white

.navbar .navbar-nav
	display inline-block
	float none
	vertical-align top
	background-color white

.navbar .navbar-collapse
	text-align center
	min-height 80px

.navbar-item
	font-size 16px
	min-height 80px
	text-transform uppercase
	text-align center
	vertical-align middle

.navbar .nav > li > a, .navbar .nav > li
	line-height 50px
	color rgba(242, 102, 143, 1)

.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a:focus 
	color rgba(242, 102, 143, 1)
	border-top-style solid
 
.active
	color rgba(242, 102, 143, 1)
	border-top-style solid

